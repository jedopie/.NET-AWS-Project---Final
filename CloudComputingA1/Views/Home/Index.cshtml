@{
    ViewData["Title"] = "Home Page";
}

@*<script>type = "text/javascript" source src = "wwwroot/js/site.js"</script>*@

<div class="text-center">
    <h1 id="head" class="display-4">Login</h1>
</div>
<div class="text-center">
    <form id="login">
        <label for="userName">Email:</label><br>
        <input type="text" id="email" name="email" placeholder="Email" required><br>
        <label for="Password">Password:</label><br>
        <input type="text" id="password" name="password" placeholder="Password" required><br>
        <div class="button">
            <button type="submit" onclick="login()">Submit</button>
        </div>
        <div class="button">
            <button type="button" value="Register" onclick="window.location.href='/Home/Register'">Register</button>
        </div>
    </form>
</div>


