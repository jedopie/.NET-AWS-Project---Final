@{
    ViewData["Title"] = "Register";
}

<div class="text-center">
    <h1 class="display-4">Register</h1>
</div>
<div class="text-center">
    <form id="register">
        <label for="Email">Email:</label><br>
        <input type="text" id="email" name="email" placeholder="Email" required><br>
        <label for="userName">Username:</label><br>
        <input type="text" id="user_name" name="user_name" placeholder="Username" required><br>
        <label for="Password">Password:</label><br>
        <input type="text" id="password" name="password" placeholder="Password" required><br>
        <div class="button">
            <button type="submit" value="Submit" onclick="register()">Register</button>
        </div>
    </form>
</div>